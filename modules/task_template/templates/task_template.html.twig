{#
/**
 * @file task_template.html.twig
 * Default theme implementation to present Task Template data.
 *
 * This template is used when viewing Task Template pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_task_template()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('task_template') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
